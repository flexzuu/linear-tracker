<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.flexzuu.linear-tracker</string>
    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key>
      <string>$$$your path$$$</string>
    </dict>
    <key>WorkingDirectory</key>
    <string>$$$fullpath to wd$$$</string>
    <key>ProgramArguments</key>
    <array>
      <string>$$$fullpath to binary$$$</string>
      <string>-token=$$$token-here$$$</string>
    </array>
    <key>StandardOutPath</key>
    <string>/tmp/linear-tracker.stdout</string>
    <key>StandardErrorPath</key>
    <string>/tmp/linear-tracker.stderr</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <!-- <key>LaunchOnlyOnce</key>        
    <true/> -->
</dict>
</plist>